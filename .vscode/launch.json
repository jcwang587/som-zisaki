{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "console": "internalConsole",
            "python": "${command:python.interpreterPath}",
            "internalConsoleOptions": "openOnSessionStart" 
        }
    ]
}
